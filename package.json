{
  "name": "fitness-journal",
  "version": "1.0.0",
  "main": "src/App.js",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.20.0",
    "npm": ">=10.7.0"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android --reset-cache --port 8081",
    "ios": "expo start --ios --reset-cache --port 8082",
    "web": "expo start --web --reset-cache --port 8083",
    "phoenix": "rm -rf .expo node_modules && npm ci",
    "clean-install": "rm -rf .expo node_modules && rm -f package-lock.json && npm i",
    "lint": "eslint .",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@expo/metro-runtime": "3.2.3",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "expo": "51.0.38",
    "i18next": "^23.15.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^15.0.1",
    "react-native": "0.74.5",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-web": "0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.37.1",
    "vitest": "^2.0.5"
  }
}
